    <com.robobunny.SeekBarPreference android:key="click_volume" 
        android:title="@string/click_volume" android:summary="@string/click_volume_summary" 
        android:defaultValue="1.0" robobunny:max="1.0" robobunny:min="0.0" 
        robobunny:unitsLeft="" robobunny:unitsRight=""/>

