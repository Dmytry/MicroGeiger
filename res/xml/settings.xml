    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:robobunny="http://robobunny.com" >
    <EditTextPreference android:title="@string/threshold" android:defaultValue="0.1" android:inputType="numberDecimal" android:key="threshold" android:persistent="true" android:summary="@string/threshold_summary"/>
